<p id="notice"><%= notice %></p>

<h1>Challange Walls</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @challange_walls.each do |challange_wall| %>
      <tr>
        <td><%= link_to 'Show', challange_wall %></td>
        <td><%= link_to 'Edit', edit_challange_wall_path(challange_wall) %></td>
        <td><%= link_to 'Destroy', challange_wall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

